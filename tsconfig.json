{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/src/*": ["src/*"],
      "@/modules/*": ["src/modules/*"],
      "@/constants": ["src/constants/index.ts"],
      "@/templates/*": ["src/templates/*"],
      "@/contexts/*": ["src/contexts/*"],
      "@/hooks/*": ["src/hooks/*"],
      "@/atoms/*": ["src/components/atoms/*"],
      "@/molecules/*": ["src/components/molecules/*"],
      "@/organisms/*": ["src/components/organisms/*"],
      "@/styles/*": ["src/styles/*"]
    }
  }
}
